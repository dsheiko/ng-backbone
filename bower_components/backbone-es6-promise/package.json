  "main": "lib/backbone-es6-promise.js",
  "author": {
    "name": "Marat Fakhreev",
    "email": "marat.fakhreev@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "backbone",
    "marionette",
    "promise",
    "override",
    "es6",
    "es2015",
    "sync",
    "model",
    "collection"
  ],
  "github": "https://github.com/maratfakhreev/backbone-es6-promise",
  "repository": {
    "type": "git",
    "url": "git://github.com/maratfakhreev/backbone-es6-promise.git"
  },
